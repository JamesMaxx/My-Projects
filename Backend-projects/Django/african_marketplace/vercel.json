{
  "version": 2,
  "builds": [
    {
      "src": "african_marketplace/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "african_marketplace/wsgi.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "DJANGO_SETTINGS_MODULE": "african_marketplace.settings"
  },
  "functions": {
    "african_marketplace/wsgi.py": {
      "includeFiles": "**"
    }
  }
}